{
    "name": "fruux/baikal",
    "description": "Baïkal is a lightweight CalDAV + CardDAV server based on PHP, SQLite and SabreDAV",
    "keywords": ["Framework", "WebDAV", "CalDAV", "CardDAV", "iCalendar"],
    "homepage": "http://sabre.io/baikal/",
    "license" : "GPL-3.0",
    "authors": [
        {
            "name": "Jérôme Schneider",
            "email": "mail@jeromeschneider.fr",
            "homepage" : "http://jerome.io",
            "role" : "Developer"
        }
    ],
    "require": {
        "php"           : ">=5.5",
        "sabre/dav"     : "~3.1.2",
        "twig/twig"     : "^1.24",
        "silex/silex": "^2.0",
        "symfony/twig-bridge": "^3.1"
    },
    "require-dev" : {
        "sabre/cs"      : "^1.0.0"
    },
    "replace" : {
        "jeromeschneider/baikal" : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Baikal\\"         : ["Core/Frameworks/Baikal/", "src/"],
            "BaikalAdmin\\"    : ["Core/Frameworks/BaikalAdmin/"],
            "Flake\\"          : ["Core/Frameworks/Flake/"],
            "Formal\\"         : ["Core/Frameworks/Formal/"]
        }
    },
    "support" : {
        "source" : "https://github.com/fruux/Baikal"
    }
}
